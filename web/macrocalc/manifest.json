{
    "name": "MacroCalc",
    "short_name": "MacroCalc",
    "start_url": "/macrocalc",
    "scope": "/macrocalc",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#6A3DE8",
    "description": "A simple macro calculator for recipes.",
    "icons": [
        {
            "src": "icon.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any maskable"
        }
    ],
    "url_handlers": [
        {
            "origin": "https://wtz.app/macrocalc"
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "web+macrocalc",
            "url": "/?recipe=%s"
        }
    ]
}